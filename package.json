{
  "name": "@ephys/ephys.github.io",
  "private": true,
  "description": "My portfolio",
  "version": "2.0.0",
  "author": "Guylian Cox <hello@guylian.me>",
  "scripts": {
    "start": "gatsby serve",
    "build": "NODE_ENV=production gatsby build && cp CNAME public/CNAME",
    "start:dev": "gatsby develop",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "serve": "gatsby serve",
    "test": "check-engines && concurrently \"npm:test:*\"",
    "test:ts-dry-run": "check-engines && tsc --noEmit --project tsconfig.json --skipLibCheck",
    "release": "npm run build && gh-pages --branch \"gh-pages\" -d public --message \"[skip ci] Deploy\" --dotfiles"
  },
  "dependencies": {
    "add": "^2.0.6",
    "classnames": "^2.3.2",
    "gatsby": "^5.11.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-manifest": "^5.11.0",
    "gatsby-plugin-react-helmet": "^6.11.0",
    "gatsby-plugin-sass": "^6.11.0",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-plugin-typescript": "^5.11.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-source-name": "^1.0.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-transformer-remark": "^6.11.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "http-server": "^14.1.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.64.2"
  },
  "devDependencies": {
    "@types/node": "^20.4.6",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "check-engines": "^1.6.0",
    "concurrently": "^8.2.0",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "react-feather": "^2.0.10",
    "typescript": "^5.1.6",
    "typescript-plugin-css-modules": "^5.0.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ephys/ephys.github.io"
  },
  "bugs": {
    "url": "https://github.com/ephys/ephys.github.io/issues"
  }
}
